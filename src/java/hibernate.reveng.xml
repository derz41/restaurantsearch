<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd">
<hibernate-reverse-engineering>
  <schema-selection match-catalog="mydb"/>
  <table-filter match-name="user"/>
  <table-filter match-name="location"/>
  <table-filter match-name="cuisine"/>
  <table-filter match-name="guest_reservation"/>
  <table-filter match-name="reservation"/>
  <table-filter match-name="guest"/>
  <table-filter match-name="restaurant"/>
</hibernate-reverse-engineering>
